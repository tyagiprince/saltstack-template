file_client: local

hash_type: sha256

log_level: info

module_dirs:
  - /srv/salt/ext

file_roots:
  base:
    - /srv/salt/base/*
  dev:
    - /srv/salt/dev/*
  stg:
    - /srv/salt/stg/*
  prd:
    - /srv/salt/prd/*

pillar_roots:
  base:
    - /srv/salt/base/pillars
  dev:
    - /srv/salt/dev/pillars
  stg:
    - /srv/salt/stg/pillars
  prd:
    - /srv/salt/prd/pillars

ext_pillar:
  - example: {}
